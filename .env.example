# Anthropic API Key (required for Claude models)
ANTHROPIC_API_KEY=sk-ant-xxxxx

# ZhipuAI API Key (optional, for GLM models)
ZHIPU_API_KEY=xxxxx

# Configuration path (optional, defaults to ./config)
CONFIG_PATH=./config

# Data path for persistence (optional, defaults to ./data)
DATA_PATH=./data

# Log level (optional: debug, info, warn, error)
LOG_LEVEL=info
